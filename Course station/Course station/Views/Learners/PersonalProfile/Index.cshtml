@model IEnumerable<Course_station.Models.PersonalProfile>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProfileId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PreferedContentType)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmotionalState)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PersonalityType)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ProfileId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PreferedContentType)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EmotionalState)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PersonalityType)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.ProfileId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.ProfileId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.ProfileId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
